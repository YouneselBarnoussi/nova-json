{
  "name": "youneselbarnoussi/nova-json",
  "description": "JSON Laravel Nova field.",
  "keywords": [
    "laravel",
    "json",
    "field",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0.0",
    "64robots/nova-fields": "^0.22"
  },
  "autoload": {
    "psr-4": {
      "Barnoussi\\NovaJson\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Barnoussi\\NovaJson\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
